l=[]
print("enter no of ele")
for i in range(int(input())):
    j=int(input())
    l.append(j)
l1=[]
m=0
n=0
f=len(l)
f1=0
for i in l:
    k=0
    for j in l:
        if i==j:
            k+=1
    if f>k:
        f=k
        m=i
    if f1<k:
        f1=k
        n=i
print("max=",m)
print("min=",n)
print("difference=",m-n)
